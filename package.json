{
  "name": "workgo-node-backend",
  "version": "1.0.0",
  "main": "src/app.ts",
  "module": "",
  "engines": {
    "node": "22.1.0"
  },
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development docker-compose up && nodemon --watch './src/schemas/shared/**/*' --ext 'ts' --exec 'npm run gen-types'",
    "docker:dev": "NODE_ENV=development nodemon --watch './src/**/*' --ext 'ts' --exec 'npm run compile'",
    "build": "NODE_ENV=development docker-compose build --no-cache",
    "compile": "tsx src/app.ts",
    "sync": "tsx scripts/sync-shared.ts",
    "format": "prettier . --write",
    "gen:random-string": "tsx scripts/generate-random-string.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/pg": "^8.11.10",
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0",
    "tsx": "^4.19.1",
    "zod-to-ts": "^1.2.0"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/swagger": "^9.2.0",
    "@fastify/swagger-ui": "^5.0.1",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@node-rs/argon2": "^2.0.0",
    "@prisma/client": "^5.20.0",
    "@t3-oss/env-core": "^0.11.1",
    "arctic": "^1.9.2",
    "fastify": "^5.0.0",
    "fastify-type-provider-zod": "^4.0.2",
    "lucia": "^3.2.0",
    "ofetch": "^1.4.1",
    "oslo": "^1.2.1",
    "zod": "^3.23.8"
  }
}
